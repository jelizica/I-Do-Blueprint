{
  "ide": {
    "hasSeenNudge": true,
    "enabled": true
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "mcpServers": {
    "adr-analysis": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-adr-analysis-server"
      ],
      "env": {
        "ADR_DIRECTORY": "docs/adrs",
        "LOG_LEVEL": "ERROR",
        "OPENROUTER_API_KEY": "${OPENROUTER_API_KEY}",
        "PROJECT_PATH": "${PROJECT_PATH}"
      }
    },
    "basic-memory": {
      "command": "uvx",
      "args": ["basic-memory", "mcp"]
    },
    "beads": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "beads-mcp"
      ]
    },
    "code-guardian": {
      "type": "stdio",
      "command": "/Users/jessicaclark/.nvm/versions/node/v20.19.6/bin/node",
      "args": [
        "/Users/jessicaclark/Development/mcp-servers/code-guardian/dist/index.js"
      ],
      "env": {
        "CCG_PROJECT_ROOT": "${PROJECT_PATH}"
      }
    },
    "greb-mcp": {
      "type": "stdio",
      "command": "greb-mcp",
      "env": {
        "GREB_API_KEY": "${GREB_API_KEY}"
      }
    },
    "local-file-organizer": {
      "type": "stdio",
      "command": "/Users/jessicaclark/Development/mcp-servers/local_file_organizer/venv/bin/python",
      "args": [
        "/Users/jessicaclark/Development/mcp-servers/local_file_organizer/file_organizer.py",
        "${PROJECT_PATH}"
      ]
    },
    "predev": {
      "type": "http",
      "url": "https://api.pre.dev/mcp"
    },
    "supabase": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.supabase.com/mcp?project_ref=pcmasfomyhqapaaaxzby"
      ]
    },
    "swiftzilla": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@swiftzilla/mcp",
        "--api-key",
        "${SWIFTZILLA_API_KEY}"
      ]
    },
    "narsil-mcp": {
      "type": "stdio",
      "command": "narsil-mcp",
      "args": [
        "--repos",
        "${PROJECT_PATH}",
        "--git",
        "--call-graph",
        "--lsp"
      ],
      "env": {
        "NARSIL_ENABLED_CATEGORIES": "Repository,Symbols,Search,CallGraph,DataFlow,Git,LSP",
        "NARSIL_DISABLED_TOOLS": "neural_search,find_semantic_clones,get_neural_stats,get_chunks,get_chunk_stats,get_embedding_stats,get_control_flow,find_dead_code,infer_types,check_type_errors,get_typed_taint_flow,get_import_graph,find_circular_imports,get_incremental_status,find_injection_vulnerabilities,trace_taint,get_security_summary,scan_security,check_owasp_top10,check_cwe_top25,explain_vulnerability,suggest_fix,generate_sbom,check_dependencies,check_licenses,find_upgrade_path,add_remote_repo,list_remote_files,get_remote_file,get_metrics"
      }
    },
    "llm-council": {
      "command": "uv",
      "args": [
        "--directory",
        "/Users/jessicaclark/Development/mcp-servers/llm-council",
        "run",
        "llm-council-mcp"
      ],
      "env": {
        "OPENROUTER_API_KEY": "${OPENROUTER_API_KEY}"
      }
    }
  }
}
